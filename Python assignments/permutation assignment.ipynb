    "    b = i\n",
    "    perm.append(b)\n",
    "print(perm)\n",
    "\n",
    "\n",
    "\n",
    "for i in range(1, l+1):\n",
    "    k = k*i\n",
    "print(f\"These values can be permutated in {k} ways\" )"
